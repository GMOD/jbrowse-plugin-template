{
  "version": "0.0.1",
  "name": "jbrowse-plugin-my-project",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "setup": "npm-run-all setup:*",
    "setup:file": "node scripts/setup.js",
    "setup:browse": "npm-run-all cleanBrowse createBrowse",
    "cleanBrowse": "rimraf .jbrowse",
    "createBrowse": "jbrowse create .jbrowse",
    "clean": "rimraf dist",
    "start": "npm-run-all --parallel watch devServer",
    "prewatch": "npm-run-all clean",
    "watch": "rollup --config --watch",
    "devServer": "serve --cors --listen 9000 .",
    "prebuild": "npm-run-all clean",
    "build": "rollup --config",
    "browse": "npm-run-all browse:*",
    "browse:setup": "shx cp jbrowse_config.json .jbrowse/config.json",
    "browse:serve": "serve .jbrowse",
    "test": "jest",
    "test:cy": "cypress run --headless --browser chrome",
    "e2e": "start-test start 9000 browse 5000 test:cy",
    "cypress": "cypress open",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src/",
    "prepublishOnly": "npm test",
    "prepare": "npm-run-all build",
    "postinstall": "jbrowse-plugin-postinstall",
    "postversion": "git push --follow-tags"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "@jbrowse/core": "^1.0.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.10.4",
    "@jbrowse/cli": "^1.0.3",
    "@jbrowse/core": "^1.0.4",
    "@jbrowse/development-tools": "^1.4.0",
    "@material-ui/core": "^4.9.13",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@types/react": "^17.0.0",
    "cypress": "^6.2.1",
    "mobx": "^5.0.0",
    "mobx-react": "^6.0.0",
    "mobx-state-tree": "3.14.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-external-globals": "^0.6.1",
    "rxjs": "^6.0.0",
    "serve": "^11.3.2",
    "shx": "^0.3.3",
    "start-server-and-test": "^1.11.7",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "private": true
}
